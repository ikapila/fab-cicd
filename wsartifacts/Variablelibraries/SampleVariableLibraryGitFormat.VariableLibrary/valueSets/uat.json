{
  "$schema": "https://developer.microsoft.com/json-schemas/fabric/item/variableLibrary/definition/valueSet/1.0.0/schema.json",
  "name": "uat",
  "description": "UAT environment settings",
  "variableOverrides": [
    {
      "name": "ApiEndpoint",
      "value": "https://uat-api.example.com/v1"
    },
    {
      "name": "MaxRetries",
      "value": "4"
    },
    {
      "name": "EnableDebugLogging",
      "value": "true"
    },
    {
      "name": "ConnectionString",
      "value": "Server=uat-server;Database=uat-db"
    }
  ]
}
